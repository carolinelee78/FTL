T13_DCN_IACCAT <- TIAS2013_IAC[, c("TA130866", "CAT")] %>% group_by(TA130866, CAT) %>% summarise(Count = n())

T13_DCN_IACCAT <- T13_DCN_IACCAT[1:5, ]


  geom_bar(stat="identity", width=1, position = "dodge") + 
  labs(title = "TIAS 2013", x = "Category", y = "Count") + 
  scale_fill_manual("WTR Involved in Arts", values =  c("lightgreen", "lightpink"), labels = c("Yes", "No"))

